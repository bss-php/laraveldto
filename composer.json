{
    "name": "bssphp/laraveldto",
    "description": "A strongly typed Data Transfer Object integration for Laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "bss",
            "email": "beanstack.systems@gmail.com",
            "homepage": "http://beanstack.in/"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "bssphp/dto": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/framework": "^9.7",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0",
        "bssphp/php-cs-fixer-config": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "bssphp\\laraveldto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bssphp\\laraveldto\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true,
        "prefer-stable":true,
        "minimum-stability":"dev"
    }
}
